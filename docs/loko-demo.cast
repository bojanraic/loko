[19.859999999999992, "o", "\r\n"]
[20.359999999999992, "o", "environment:\r\n"]
[20.379999999999995, "o", "  # General settings\r\n"]
[20.4, "o", "  name: dev-me # name of the environment; drives the naming of kubernetes cluster and nodes, host containers etc\r\n"]
[20.42, "o", "\r\n"]
[20.440000000000005, "o", "  # NOTE: Authentication and persistence are handled automatically for system services\r\n"]
[20.460000000000008, "o", "  # - Random passwords are generated for all database services\r\n"]
[20.48000000000001, "o", "  # - Persistence is configured based on storage.size settings\r\n"]
[20.500000000000014, "o", "  # - Passwords can be retrieved using: task kubernetes:fetch-service-secrets\r\n"]
[20.520000000000017, "o", "  base-dir: ${PWD}/.loko # where kubernetes logs/storage, and various config files are stored\r\n"]
[20.54000000000002, "o", "  expand-env-vars: true # set to false to disable variable expansion; true to enable expansion of:\r\n"]
[20.560000000000024, "o", "                        # - OS variables (${PWD}, ${HOME}, ${USER}) in base-dir and helm values\r\n"]
[20.580000000000027, "o", "                        # - loko variables (${LOCAL_DOMAIN}, ${ENV_NAME}, etc.) in helm values\r\n"]
[20.60000000000003, "o", "                        # Available loko variables for use in helm values:\r\n"]
[20.620000000000033, "o", "                        # - ${ENV_NAME}         - environment name (e.g., \"dev-me\")\r\n"]
[20.640000000000036, "o", "                        # - ${APPS_SUBDOMAIN}   - subdomain for applications (e.g., \"apps\")\r\n"]
[20.66000000000004, "o", "                        # - ${LOCAL_DOMAIN}     - local domain (e.g., \"dev.me\")\r\n"]
[20.680000000000042, "o", "                        # - ${LOCAL_APPS_DOMAIN}- domain for applications, either APPS_SUBDOMAIN.LOCAL_DOMAIN or LOCAL_DOMAIN\r\n"]
[20.700000000000045, "o", "                        #                        depending on use-apps-subdomain setting (e.g., \"app.dev.me\" or \"dev.me\")\r\n"]
[20.72000000000005, "o", "                        # - ${LOCAL_IP}         - local IP address (e.g., \"192.168.0.10\")\r\n"]
[20.74000000000005, "o", "                        # - ${REGISTRY_NAME}    - registry name (e.g., \"cr\")\r\n"]
[20.760000000000055, "o", "                        # - ${REGISTRY_HOST}    - registry host (e.g., \"cr.dev.me\")\r\n"]
[20.780000000000058, "o", "\r\n"]
[20.80000000000006, "o", "  # At the moment, KinD on macOS has been tested, with a docker-compatible runtime\r\n"]
[20.820000000000064, "o", "  # Podman has been tried out but it is not a first-class citizen with KinD so it is not supported as of yet\r\n"]
[20.840000000000067, "o", "  # Linux has not been tested yet\r\n"]
[20.86000000000007, "o", "  provider:\r\n"]
[20.880000000000074, "o", "    name: kind # provider for kubernetes clusters, must be kind for now\r\n"]
[20.900000000000077, "o", "    runtime: docker # docker or podman for container runtime\r\n"]
[20.92000000000008, "o", "\r\n"]
[20.940000000000083, "o", "  kubernetes:\r\n"]
[20.960000000000086, "o", "    api-port: 6443 # port for the API server, will be exposed to the host machine as local-ip:api-port\r\n"]
[20.98000000000009, "o", "    image: kindest/node # image for the kind nodes\r\n"]
[21.000000000000092, "o", "    # renovate: datasource=docker depName=kindest/node\r\n"]
[21.020000000000095, "o", "    tag: v1.34.0  # tag for the kind image\r\n"]
[21.0400000000001, "o", "\r\n"]
[21.0600000000001, "o", "  nodes: # common settings for all kubernetes nodes\r\n"]
[21.080000000000105, "o", "    servers: 1 # number of control-plane servers/masters\r\n"]
[21.100000000000108, "o", "    workers: 1 # number of worker nodes\r\n"]
[21.12000000000011, "o", "    allow-scheduling-on-control-plane: true # whether to allow scheduling on control plane nodes even when workers exist\r\n"]
[21.140000000000114, "o", "    internal-components-on-control-plane: true # whether to force internal components (nginx-ingress, registry) to run only on control-plane nodes\r\n"]
[21.160000000000117, "o", "\r\n"]
[21.18000000000012, "o", "    labels: # optional: custom labels to apply to nodes\r\n"]
[21.200000000000124, "o", "      control-plane: # labels for ALL control-plane nodes (applied to each)\r\n"]
[21.220000000000127, "o", "        tier: \"control\"\r\n"]
[21.24000000000013, "o", "        environment: \"dev\"\r\n"]
[21.260000000000133, "o", "      worker: # labels for ALL worker nodes (applied to each)\r\n"]
[21.280000000000136, "o", "        tier: \"compute\"\r\n"]
[21.30000000000014, "o", "        environment: \"dev\"\r\n"]
[21.320000000000142, "o", "      # Alternative: specify labels per individual node (overrides global labels)\r\n"]
[21.340000000000146, "o", "      # individual:\r\n"]
[21.36000000000015, "o", "      #   control-plane-0: # labels for first control-plane node\r\n"]
[21.38000000000015, "o", "      #     tier: \"control\"\r\n"]
[21.400000000000155, "o", "      #     zone: \"us-west-1a\"\r\n"]
[21.420000000000158, "o", "      #   control-plane-1: # labels for second control-plane node\r\n"]
[21.44000000000016, "o", "      #     tier: \"control\"\r\n"]
[21.460000000000164, "o", "      #     zone: \"us-west-1b\"\r\n"]
[21.480000000000167, "o", "      #   worker-0: # labels for first worker node\r\n"]
[21.50000000000017, "o", "      #     tier: \"compute\"\r\n"]
[21.520000000000174, "o", "      #     zone: \"us-west-1a\"\r\n"]
[21.540000000000177, "o", "      #   worker-1: # labels for second worker node\r\n"]
[21.56000000000018, "o", "      #     tier: \"compute\"\r\n"]
[21.580000000000183, "o", "      #     zone: \"us-west-1b\"\r\n"]
[21.600000000000186, "o", "\r\n"]
[21.62000000000019, "o", "  local-ip: 192.168.1.120 # local ip, mapping to the host IP to use for DNS resolution and wildcard certificates\r\n"]
[21.640000000000192, "o", "  local-domain: dev.me # a domain name to use for custom dns resolution and wildcard certificates\r\n"]
[21.660000000000196, "o", "  use-apps-subdomain: true # whether to use apps subdomain for applications (false = use direct domain)\r\n"]
[21.6800000000002, "o", "  apps-subdomain: apps # subdomain for applications (e.g., myapp.apps.dev.me)\r\n"]
[21.700000000000202, "o", "  local-lb-ports: # list of ports to expose on the load balancer side, mapping to the host machine\r\n"]
[21.720000000000205, "o", "    - 80 # http port for nginx ingress controller\r\n"]
[21.740000000000208, "o", "    - 443 # https port for nginx ingress controller\r\n"]
[21.76000000000021, "o", "\r\n"]
[21.780000000000214, "o", "  registry:\r\n"]
[21.800000000000217, "o", "    name: cr # name, to be used in the final url for the registry, i.e. <registry.name>.<local-domain>\r\n"]
[21.82000000000022, "o", "    storage: # use PVC for storage\r\n"]
[21.840000000000224, "o", "      size: 10Gi # size of PVC\r\n"]
[21.860000000000227, "o", "\r\n"]
[21.88000000000023, "o", "  internal-components:\r\n"]
[21.900000000000233, "o", "    # renovate: datasource=helm depName=app-template repositoryUrl=https://bjw-s-labs.github.io/helm-charts\r\n"]
[21.920000000000236, "o", "    - app-template: \"4.4.0\"\r\n"]
[21.94000000000024, "o", "\r\n"]
[21.960000000000242, "o", "    # renovate: datasource=helm depName=traefik repositoryUrl=https://traefik.github.io/charts\r\n"]
[21.980000000000246, "o", "    - traefik: \"37.4.0\"\r\n"]
[22.00000000000025, "o", "\r\n"]
[22.020000000000252, "o", "    # renovate: datasource=helm depName=metrics-server repositoryUrl=https://kubernetes-sigs.github.io/metrics-server\r\n"]
[22.040000000000255, "o", "    - metrics-server: \"3.13.0\"\r\n"]
[22.060000000000258, "o", "\r\n"]
[22.08000000000026, "o", "    # renovate: datasource=docker depName=registry\r\n"]
[22.100000000000264, "o", "    - registry: \"3\"\r\n"]
[22.120000000000267, "o", "\r\n"]
[22.14000000000027, "o", "    # renovate: datasource=docker depName=dockurr/dnsmasq\r\n"]
[22.160000000000274, "o", "    - dnsmasq: \"2.91\"\r\n"]
[22.180000000000277, "o", "\r\n"]
[22.20000000000028, "o", "  use-service-presets: true # whether or not to use the preset values for services; leave true unless you have a good reason to override the defaults\r\n"]
[22.220000000000283, "o", "  run-services-on-workers-only: true # whether to force application services to run only on worker nodes (when workers > 0)\r\n"]
[22.240000000000286, "o", "  enable-metrics-server: false # whether to deploy metrics-server for resource monitoring and HPA\r\n"]
[22.26000000000029, "o", "\r\n"]
[22.280000000000292, "o", "  # Centralized helm repository definitions\r\n"]
[22.300000000000296, "o", "  helm-repositories:\r\n"]
[22.3200000000003, "o", "    - name: groundhog2k\r\n"]
[22.340000000000302, "o", "      url: https://groundhog2k.github.io/helm-charts/\r\n"]
[22.360000000000305, "o", "    - name: securecodebox\r\n"]
[22.380000000000308, "o", "      url: https://charts.securecodebox.io/\r\n"]
[22.40000000000031, "o", "\r\n"]
[22.420000000000314, "o", "  services:\r\n"]
[22.440000000000317, "o", "    # System services - pre-configured services with presets, ports, and storage management\r\n"]
[22.46000000000032, "o", "    system:\r\n"]
[22.480000000000324, "o", "      - name: mysql\r\n"]
[22.500000000000327, "o", "        enabled: false # whether or not to install this component and expose the port(s)\r\n"]
[22.52000000000033, "o", "        namespace: common-services\r\n"]
[22.540000000000333, "o", "        ports:\r\n"]
[22.560000000000336, "o", "          - 3306 # port to expose on the host machine\r\n"]
[22.58000000000034, "o", "        storage: # use PVC for storage\r\n"]
[22.600000000000342, "o", "          size: 10Gi # size of PVC\r\n"]
[22.620000000000346, "o", "        config:\r\n"]
[22.64000000000035, "o", "          repo:\r\n"]
[22.660000000000352, "o", "            ref: groundhog2k # reference to helm-repositories entry\r\n"]
[22.680000000000355, "o", "          chart: groundhog2k/mysql # the chart to use\r\n"]
[22.700000000000358, "o", "          # renovate: datasource=helm depName=mysql repositoryUrl=https://groundhog2k.github.io/helm-charts\r\n"]
[22.72000000000036, "o", "          version: 3.0.7\r\n"]
[22.740000000000364, "o", "          # MySQL authentication and persistence handled automatically\r\n"]
[22.760000000000367, "o", "\r\n"]
[22.78000000000037, "o", "      - name: postgres\r\n"]
[22.800000000000374, "o", "        enabled: true # whether or not to install this component and expose the port(s)\r\n"]
[22.820000000000377, "o", "        namespace: common-services\r\n"]
[22.84000000000038, "o", "        ports:\r\n"]
[22.860000000000383, "o", "          - 5432\r\n"]
[22.880000000000386, "o", "        storage: # use PVC for storage\r\n"]
[22.90000000000039, "o", "          size: 5Gi\r\n"]
[22.920000000000393, "o", "        config:\r\n"]
[22.940000000000396, "o", "          repo:\r\n"]
[22.9600000000004, "o", "            ref: groundhog2k # reference to helm-repositories entry\r\n"]
[22.980000000000402, "o", "          chart: groundhog2k/postgres # the chart to use\r\n"]
[23.000000000000405, "o", "          # renovate: datasource=helm depName=postgres repositoryUrl=https://groundhog2k.github.io/helm-charts\r\n"]
[23.020000000000408, "o", "          version: 1.6.0\r\n"]
[23.04000000000041, "o", "          # PostgreSQL authentication and persistence handled automatically\r\n"]
[23.060000000000414, "o", "\r\n"]
[23.080000000000418, "o", "      - name: mongodb\r\n"]
[23.10000000000042, "o", "        enabled: false # whether or not to install this component and expose the port(s)\r\n"]
[23.120000000000424, "o", "        namespace: common-services\r\n"]
[23.140000000000427, "o", "        ports:\r\n"]
[23.16000000000043, "o", "          - 27017 # port to expose on the host machine\r\n"]
[23.180000000000433, "o", "        storage: # use PVC for storage\r\n"]
[23.200000000000436, "o", "          size: 5Gi # size of PVC\r\n"]
[23.22000000000044, "o", "        config:\r\n"]
[23.240000000000443, "o", "          repo:\r\n"]
[23.260000000000446, "o", "            ref: groundhog2k # reference to helm-repositories entry\r\n"]
[23.28000000000045, "o", "          chart: groundhog2k/mongodb # the chart to use\r\n"]
[23.300000000000452, "o", "          # renovate: datasource=helm depName=mongodb repositoryUrl=https://groundhog2k.github.io/helm-charts\r\n"]
[23.320000000000455, "o", "          version: 0.7.6\r\n"]
[23.340000000000458, "o", "          # MongoDB authentication and persistence handled automatically\r\n"]
[23.36000000000046, "o", "\r\n"]
[23.380000000000464, "o", "      - name: rabbitmq\r\n"]
[23.400000000000468, "o", "        enabled: false # whether or not to install this component and expose the port(s)\r\n"]
[23.42000000000047, "o", "        namespace: common-services\r\n"]
[23.440000000000474, "o", "        ports:\r\n"]
[23.460000000000477, "o", "          - 5672 # port to expose on the host machine\r\n"]
[23.48000000000048, "o", "        storage: # use PVC for storage\r\n"]
[23.500000000000483, "o", "          size: 2Gi # size of PVC\r\n"]
[23.520000000000486, "o", "        config:\r\n"]
[23.54000000000049, "o", "          repo:\r\n"]
[23.560000000000493, "o", "            ref: groundhog2k # reference to helm-repositories entry\r\n"]
[23.580000000000496, "o", "          chart: groundhog2k/rabbitmq # the chart to use\r\n"]
[23.6000000000005, "o", "          # renovate: datasource=helm depName=rabbitmq repositoryUrl=https://groundhog2k.github.io/helm-charts\r\n"]
[23.620000000000502, "o", "          version: 2.2.2\r\n"]
[23.640000000000505, "o", "          values:\r\n"]
[23.660000000000508, "o", "            # RabbitMQ authentication handled automatically\r\n"]
[23.68000000000051, "o", "            ingress:\r\n"]
[23.700000000000514, "o", "              enabled: true\r\n"]
[23.720000000000518, "o", "              hostname: rmq.${LOCAL_DOMAIN}\r\n"]
[23.74000000000052, "o", "              annotations:\r\n"]
[23.760000000000524, "o", "                traefik.ingress.kubernetes.io/router.entrypoints: websecure\r\n"]
[23.780000000000527, "o", "                traefik.ingress.kubernetes.io/router.tls: \"true\"\r\n"]
[23.80000000000053, "o", "              tls:\r\n"]
[23.820000000000533, "o", "                - hosts:\r\n"]
[23.840000000000536, "o", "                    - rmq.${LOCAL_DOMAIN}\r\n"]
[23.86000000000054, "o", "\r\n"]
[23.880000000000543, "o", "      - name: valkey\r\n"]
[23.900000000000546, "o", "        enabled: false # whether or not to install this component and expose the port(s)\r\n"]
[23.92000000000055, "o", "        namespace: common-services\r\n"]
[23.940000000000552, "o", "        ports:\r\n"]
[23.960000000000555, "o", "          - 6379 # port to expose on the host machine\r\n"]
[23.980000000000558, "o", "        storage:\r\n"]
[24.00000000000056, "o", "          size: 1Gi\r\n"]
[24.020000000000564, "o", "        config:\r\n"]
[24.040000000000568, "o", "          repo:\r\n"]
[24.06000000000057, "o", "            ref: groundhog2k # reference to helm-repositories entry\r\n"]
[24.080000000000574, "o", "          chart: groundhog2k/valkey # the chart to use\r\n"]
[24.100000000000577, "o", "          # renovate: datasource=helm depName=valkey repositoryUrl=https://groundhog2k.github.io/helm-charts\r\n"]
[24.12000000000058, "o", "          version: 2.2.2\r\n"]
[24.140000000000583, "o", "          # Valkey persistence handled automatically (no authentication)\r\n"]
[24.160000000000586, "o", "\r\n"]
[24.18000000000059, "o", "    # User services - additional services users can add without presets\r\n"]
[24.200000000000593, "o", "    # Users are expected to provide:\r\n"]
[24.220000000000596, "o", "    #  - complete helm values configuration\r\n"]
[24.2400000000006, "o", "    #  - helm repository configuration (required)\r\n"]
[24.260000000000602, "o", "    #  - ports they want to have exposed on the host machine (optional)\r\n"]
[24.280000000000605, "o", "    #\r\n"]
[24.30000000000061, "o", "    # DNS and Ingress Structure:\r\n"]
[24.32000000000061, "o", "    # - System services use: service.local.domain (e.g., mysql.dev.me)\r\n"]
[24.340000000000614, "o", "    # - User services use: service.apps.local.domain (e.g., myapp.apps.dev.me)\r\n"]
[24.360000000000618, "o", "    # - Registry uses: registry.local.domain (e.g., cr.dev.me)\r\n"]
[24.38000000000062, "o", "    # This separation provides better security by isolating system services from user applications\r\n"]
[24.400000000000624, "o", "\r\n"]
[24.420000000000627, "o", "    user:\r\n"]
[24.44000000000063, "o", "      - name: http-webhook\r\n"]
[24.460000000000633, "o", "        enabled: false\r\n"]
[24.480000000000636, "o", "        namespace: default\r\n"]
[24.50000000000064, "o", "        ports: [] # Optional: open up ports on host machine\r\n"]
[24.520000000000643, "o", "        config:\r\n"]
[24.540000000000646, "o", "          repo: # repo is required\r\n"]
[24.56000000000065, "o", "            ref: securecodebox # reference to helm-repositories entry\r\n"]
[24.580000000000652, "o", "          chart: securecodebox/http-webhook\r\n"]
[24.600000000000655, "o", "          # renovate: datasource=helm depName=http-webhook repositoryUrl=https://charts.securecodebox.io\r\n"]
[24.62000000000066, "o", "          version: 5.2.0\r\n"]
[24.64000000000066, "o", "          values:\r\n"]
[24.660000000000664, "o", "            ingress:\r\n"]
[24.680000000000668, "o", "              enabled: true\r\n"]
[24.70000000000067, "o", "              ingressClassName: \"traefik\"\r\n"]
[24.720000000000674, "o", "              annotations:\r\n"]
[24.740000000000677, "o", "                traefik.ingress.kubernetes.io/router.entrypoints: websecure\r\n"]
[24.76000000000068, "o", "                traefik.ingress.kubernetes.io/router.tls: \"true\"\r\n"]
